<li class="menu-tag" draggable="true" menu-num="{{menuNum}}" style="top: {{y}}; left: {{x}}">
    <div class="menu-tag-cage">
        <input class="menu-input-text" maxlength="20" type="text" value="{{menu}}">
        <input class="menu-input-number" maxlength="10" type="number" value="{{price}}">
        <i class="menu-btn-delete fa fa-minus"></i>
    </div>
</li>